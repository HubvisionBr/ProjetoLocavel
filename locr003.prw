Local CCNPJ			:= ""
Local CEND			:= ""
Local CBAIRRO		:= ""
Local CCIDADE		:= ""
Local CCEP			:= ""
Local CUF			:= ""
Local CTEL			:= ""
Local CFAX			:= ""
Local cAliasSc6		:= GetNextAlias()
Local cPeriodo  	:= ""
Local cChave 		:= ""
Local CLOGO			:= ""
Local CARQUIVO   	:= ""
Local _CQUERY      	:= ""
Local cAsPedido		:= ""
Local I           	:= 0
