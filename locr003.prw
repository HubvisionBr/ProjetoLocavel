	//CLOGO := GETSRVPROFSTRING("STARTPATH","") + "LOGO.JPG"
	CLOGO := GETSRVPROFSTRING("STARTPATH","") + "lgmid" + cDescLogo + ".jpg"
	//AJUSTE PARA SAIR O LOGO POR FILIAL LOGADA NO SISTEMA - DENNIS CARD 1334 - FIM

	IF SELECT("TRBFAT") > 0
