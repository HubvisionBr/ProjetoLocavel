	//Montando a consulta dos pedidos
	oStatement := FWPreparedStatement():New()

	// --> (A) CORRECAO DO VALOR DOS IMPOSTOS RETIDOS NA EMISSAO DO RELATORIO.   (*INICIO*)
	_CQUERY := "SELECT "
	_CQUERY += "(SELECT SUM(E1_VALOR) FROM "+RETSQLNAME("SE1")+" SE1TMP1 (NOLOCK) WHERE SE1TMP1.E1_FILIAL = '"+XFILIAL("SE1")+"' AND XTEMP1.NUMFAT = SE1TMP1.E1_NUM AND XTEMP1.SERIE=SE1TMP1.E1_PREFIXO AND SE1TMP1.E1_TIPO IN ('IR-','CS-','PI-','CF-','IS-','IN-') AND SE1TMP1.D_E_L_E_T_ = '') AS 'VALRET2', "
	_CQUERY += "XTEMP1.* "
	_CQUERY += "FROM "
	_CQUERY += "( "
	//_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(ISNULL(F4_TEXTO,'"+STR0004+"')) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS' // inject 1
	//_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(ISNULL(F4_TEXTO,?)) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS'
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23
	_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(COALESCE(F4_TEXTO,?)) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS'
	_CQUERY += " 	   RTRIM(LTRIM(A1_NOME)) RAZSOC, A1_END A1_END, A1_BAIRRO, A1_MUN, A1_EST, "
	_CQUERY += " 	   SUBSTRING(A1_CEP,1,5) + '-' + SUBSTRING(A1_CEP,6,3) A1_CEP, "
	_CQUERY += " 	   SUBSTRING(A1_CGC,1,2) + '.' + SUBSTRING(A1_CGC,3,3) + '.' + SUBSTRING(A1_CGC,6,3) + '/' +"
	_CQUERY += " 	   SUBSTRING(A1_CGC,9,4) + '-' + SUBSTRING(A1_CGC,13,2) A1_CNPJ, "
	_CQUERY += " 	   CASE WHEN A1_INSCR = ''"
	_CQUERY += " 			THEN 'ISENTO'"
	_CQUERY += " 			ELSE A1_INSCR   END A1_INSCR   , "
	_CQUERY += "       SE1NF.E1_VENCTO VENCTO,"
	//_CQUERY += " 	   F2_VALBRUT, SE1NF.E1_DESCONT, SUM(ISNULL(SE1IMP.E1_VALOR,0)) VALRET,"
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23
	_CQUERY += " 	   F2_VALBRUT, SE1NF.E1_DESCONT, SUM(COALESCE(SE1IMP.E1_VALOR,0)) VALRET,"
	//_CQUERY += " 	   F2_VALBRUT - ISNULL(SE1NF.E1_DESCONT,0) - SUM(ISNULL(SE1IMP.E1_VALOR,0)) - SE1NF.E1_IRRF TOTAL,"
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23
	_CQUERY += " 	   F2_VALBRUT - COALESCE(SE1NF.E1_DESCONT,0) - SUM(COALESCE(SE1IMP.E1_VALOR,0)) - SE1NF.E1_IRRF TOTAL,"
	_CQUERY += " 	   CASE WHEN A1_ENDCOB = ''"
	_CQUERY += " 			THEN A1_END"
	_CQUERY += " 			ELSE A1_ENDCOB  END A1_ENDCOB  , "
	_CQUERY += " 	   CASE WHEN A1_BAIRROC = ''"
	_CQUERY += " 			THEN A1_BAIRRO"
	_CQUERY += " 			ELSE A1_BAIRROC END A1_BAIRROC , "
	_CQUERY += " 	   CASE WHEN A1_MUNC = ''"
	_CQUERY += " 			THEN A1_MUN"
	_CQUERY += " 			ELSE A1_MUNC    END A1_MUNC    , "
	_CQUERY += " 	   CASE WHEN A1_ESTC = ''"
	_CQUERY += " 			THEN A1_EST"
	_CQUERY += " 			ELSE A1_ESTC    END A1_ESTC    , "
	_CQUERY += " 	   CASE WHEN A1_CEPC = ''"
	_CQUERY += " 			THEN SUBSTRING(A1_CEP,1,5) + '-' + SUBSTRING(A1_CEP,6,3)"
	_CQUERY += " 			ELSE SUBSTRING(A1_CEPC,1,5) + '-' + SUBSTRING(A1_CEPC,6,3) END A1_CEPC,"
	_CQUERY += " 	   CASE WHEN A1_ENDENT = ''"
	_CQUERY += " 			THEN A1_END"
	_CQUERY += " 			ELSE A1_ENDENT  END A1_ENDENT  , "
	_CQUERY += " 	   CASE WHEN A1_BAIRROE = ''"
	_CQUERY += " 			THEN A1_BAIRRO"
	_CQUERY += " 			ELSE A1_BAIRROE END A1_BAIRROE , "
	_CQUERY += " 	   CASE WHEN A1_MUNE = ''"
	_CQUERY += " 			THEN A1_MUN"
	_CQUERY += " 			ELSE A1_MUNE    END A1_MUNE    , "
	_CQUERY += " 	   CASE WHEN A1_ESTE = ''"
	_CQUERY += " 			THEN A1_EST"
	_CQUERY += " 			ELSE A1_ESTE    END A1_ESTE    , "
	_CQUERY += " 	   CASE WHEN A1_CEPE = ''"
	_CQUERY += " 			THEN SUBSTRING(A1_CEP,1,5) + '-' + SUBSTRING(A1_CEP,6,3)"
	_CQUERY += " 			ELSE SUBSTRING(A1_CEPE,1,5) + '-' + SUBSTRING(A1_CEPE,6,3) END A1_CEPE,"
	//_CQUERY += " 		ISNULL(A6_NOME,'') BANCO, "
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23 - INICIO
	_CQUERY += " 		COALESCE(A6_NOME, '') BANCO, "
	//_CQUERY += " 		CASE WHEN ISNULL(A6_DVAGE,'') = ''"
	_CQUERY += " 		CASE WHEN COALESCE(A6_DVAGE, '') = ''"
	//_CQUERY += " 		     THEN ISNULL(A6_AGENCIA,'')"
	_CQUERY += " 		     THEN COALESCE(A6_AGENCIA, '')"
	_CQUERY += " 		     ELSE RTRIM(LTRIM(A6_AGENCIA)) + '-' + RTRIM(LTRIM(A6_DVAGE)) END AGENCIA,"
	//_CQUERY += " 		CASE WHEN ISNULL(A6_DVCTA,'') = ''"
	_CQUERY += " 		CASE WHEN COALESCE(A6_DVCTA, '') = ''"
	//_CQUERY += " 		     THEN ISNULL(A6_NUMCON,'')"
	_CQUERY += " 		     THEN COALESCE(A6_NUMCON, '')"
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23 - FIM
	_CQUERY += " 		     ELSE RTRIM(LTRIM(A6_NUMCON)) + '-' + RTRIM(LTRIM(A6_DVCTA)) END CONTA, SE1NF.E1_IRRF AS IRFAT"
	_CQUERY += " FROM " + RETSQLNAME("SF2") + " SF2 "
	_CQUERY += "        INNER JOIN " + RETSQLNAME("SA1") + " SA1    ON  SF2.F2_CLIENTE = A1_COD       AND  SF2.F2_LOJA = SA1.A1_LOJA "
	_CQUERY += "                                                    AND SA1.D_E_L_E_T_ = '' "
	If !empty(alltrim(xFilial("SA1")))
		_cQuery += " AND SA1.A1_FILIAL = '"+xFilial("SA1")+"' "
	EndIF
	_CQUERY += "        INNER JOIN " + RETSQLNAME("SD2") + " SD2    ON  SD2.D2_FILIAL  = F2_FILIAL    AND  SD2.D2_DOC  = F2_DOC "
	_CQUERY += "                                                    AND SD2.D2_SERIE   = F2_SERIE "
	_CQUERY += "                                                    AND SD2.D_E_L_E_T_ = '' "
	_CQUERY += " 	    INNER JOIN " + RETSQLNAME("SC6") + " SC6    ON  SC6.C6_FILIAL  = F2_FILIAL    AND  SC6.C6_NOTA = F2_DOC "
	_CQUERY += "                                                    AND SC6.C6_SERIE   = F2_SERIE     AND  SC6.C6_ITEM = D2_ITEM "
	_CQUERY += "                                                    AND SC6.D_E_L_E_T_ = '' "
	_CQUERY += "        LEFT  JOIN " + RETSQLNAME("SF4") + " SF4    ON  SF4.F4_CODIGO  = D2_TES "
	_CQUERY += "                                                    AND SF4.D_E_L_E_T_ = '' "
	_CQUERY += "        LEFT  JOIN " + RETSQLNAME("SE1") + " SE1NF  ON  F2_FILIAL = SE1NF.E1_FILIAL   AND  F2_DOC = SE1NF.E1_NUM "
	_CQUERY += "                                                    AND F2_SERIE  = SE1NF.E1_PREFIXO "
	_CQUERY += "                                                    AND SE1NF.E1_TIPO  = 'NF' "
	_CQUERY += "                                                    AND SE1NF.D_E_L_E_T_ = '' "
	_CQUERY += " 	    LEFT  JOIN " + RETSQLNAME("SE1") + " SE1IMP ON  F2_FILIAL = SE1IMP.E1_FILIAL  AND  F2_DOC = SE1IMP.E1_NUM "
	_CQUERY += "                                                    AND F2_SERIE  = SE1IMP.E1_PREFIXO "
	//_CQUERY+="                                                    AND SE1IMP.E1_TIPO IN ('-IR','-CS','-PI','-CF','-IS','-IN')"
	_CQUERY += "                                                    AND SE1IMP.E1_TIPO IN ('IR-','CS-','PI-','CF-','IS-','IN-')"
	_CQUERY += "                                                    AND SE1IMP.D_E_L_E_T_ = '' "
	_CQUERY += " 	    LEFT  JOIN " + RETSQLNAME("SA6") + " SA6    ON  SA6.A6_FILIAL  = SE1NF.E1_FILIAL "
	_CQUERY += "                                                    AND SA6.A6_COD     = SE1NF.E1_PORTADO "
	_CQUERY += "                                                    AND SA6.A6_AGENCIA = SE1NF.E1_AGEDEP "
	_CQUERY += "                                                    AND SA6.A6_NUMCON  = SE1NF.E1_CONTA "
	_CQUERY += "                                                    AND SA6.D_E_L_E_T_ = '' "
	_CQUERY += " WHERE  F2_FILIAL = '" + XFILIAL("SF2") + "'"
	//_CQUERY += "   AND  F2_DOC BETWEEN '" + MV_PAR01 + "' AND '" + MV_PAR02 + "'"
	_CQUERY += "   AND  F2_DOC BETWEEN ? AND ? "  // inject 2 e 3
	//_CQUERY += "   AND  F2_SERIE = '" + MV_PAR03 + "'"
	_CQUERY += "   AND  F2_SERIE = ? " // inject 4
	_CQUERY += "   AND  SF2.D_E_L_E_T_ = ''"
	_CQUERY += "   AND  SF2.F2_TIPO NOT IN ('D','B') "

	_CQUERY += "  GROUP BY F2_FILIAL , F2_SERIE, F2_DOC, F2_EMISSAO, A1_NOME, A1_END, A1_BAIRRO, A1_MUN, A1_EST, A1_CEP, A1_CGC, A1_INSCR, "
	_CQUERY += " 		  SE1NF.E1_VENCTO, F2_VALBRUT, SE1NF.E1_DESCONT, A1_ENDCOB, A1_BAIRROC, A1_MUNC, A1_ESTC, A1_CEPC,"
	_CQUERY += " 		  A1_ENDENT, A1_BAIRROE, A1_MUNE, A1_ESTE, A1_CEPE, A6_NOME, A6_DVAGE, A6_AGENCIA, A6_NUMCON, A6_DVCTA,SE1NF.E1_IRRF"

	// --> (B) CORRECAO DO VALOR DOS IMPOSTOS RETIDOS NA EMISSAO DO RELATORIO.   (*INICIO*)
	_CQUERY += " UNION ALL "

	//_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(ISNULL(F4_TEXTO,'"+STR0004+"')) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS'
	//_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(ISNULL(F4_TEXTO,?)) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS' // inject 5
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23
	_CQUERY += " SELECT F2_FILIAL FILIAL, F2_SERIE SERIE, F2_DOC NUMFAT, F2_EMISSAO EMISSAO, MAX(COALESCE(F4_TEXTO,?)) F4_TEXTO, MAX(C6_NUM) PEDIDO,"  //'LOCAÇÃO DE BENS MÓVEIS' // inject 5
	_CQUERY += " 	   RTRIM(LTRIM(A2_NOME)) RAZSOC, A2_END A1_END, A2_BAIRRO A1_BAIRRO, A2_MUN A1_MUN, A2_EST A1_EST, "
	_CQUERY += " 	   SUBSTRING(A2_CEP,1,5) + '-' + SUBSTRING(A2_CEP,6,3) A1_CEP, "
	_CQUERY += " 	   SUBSTRING(A2_CGC,1,2) + '.' + SUBSTRING(A2_CGC,3,3) + '.' + SUBSTRING(A2_CGC,6,3) + '/' +"
	_CQUERY += " 	   SUBSTRING(A2_CGC,9,4) + '-' + SUBSTRING(A2_CGC,13,2) A1_CNPJ, "
	_CQUERY += " 	   CASE WHEN A2_INSCR = ''"
	_CQUERY += " 			THEN 'ISENTO'"
	_CQUERY += " 			ELSE A2_INSCR END A2_INSCR, SE1NF.E1_VENCTO VENCTO,"
	//CARD 1057 - ALTERADO DE ISNULL PARA COALESCE - DENNIS 21/12/23 - INICIO
	//_CQUERY += " 	   F2_VALBRUT, SE1NF.E1_DESCONT, SUM(ISNULL(SE1IMP.E1_VALOR,0)) VALRET,"
